generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Comment {
  id         Int      @id @default(autoincrement())
  authorId   String
  authorName String
  movieId    Int
  content    String   @db.VarChar(500)
  createdAt  DateTime @default(now())
  updatedAt  DateTime
}
